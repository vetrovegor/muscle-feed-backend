model Team {
    id            Int      @id @default(autoincrement())
    picture       String
    roleRu        String
    roleHe        String
    nameRu        String
    nameHe        String
    descriptionRu String
    descriptionHe String
    createdAt     DateTime @default(now()) @map("created_at")
    updatedAt     DateTime @updatedAt @map("updated_at")

    @@map("team")
}
